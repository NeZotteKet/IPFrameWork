package com.test.swing1.mavenspringwebapplication.converters;

import com.mycompany.myfirstmavenproject.domain.BierCategorie;
import org.springframework.core.convert.converter.Converter;

/**
 *
 * @author thomas
 */
public class CategorieConverter implements Converter<String,BierCategorie> {

    @Override
    public BierCategorie convert(String s) {
        BierCategorie bc = new BierCategorie();
        bc.setNaam(s);
        return bc;
    }

    public CategorieConverter() {
    }
    
    

   
    
}
